[core]
    excludesFile = ~/.gitignore
	eol = lf
	autocrlf = false
[color]
    ui = true
    diff = true
[alias]
    co = checkout
    in = init
    br = branch
    st = status
    ci = commit
    fe = fetch
    pu = pull
[push]
    default = simple
[fetch]
    prune = true
[diff]
    tool = vimdiff
    guitool = p4merge
[difftool "p4merge"]
    cmd = p4merge "$LOCAL" "$REMOTE"
    trustExitCode = false
[merge]
    tool = p4merge
[mergetool "p4merge"]
    cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
    trustExitCode = false
    leepBackup = false
[init]
	defaultBranch = main
[fsck]
	zeroPaddedFilemode = ignore
[fetch "fsck"]
	zeroPaddedFilemode = ignore
[receive "fsck"]
	zeroPaddedFilemode = ignore
